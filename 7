def std_weight (height, gender):
    if gender == 'Female':
        weight = height/100*height/100*21
    else: 
        weight = height/100*height/100*22
    return weight, height, gender



data = std_weight(173, 'Male')
print(data)
print(type(data))

print('키 {1}cm {2}의 표준체중은 {0:.2f}kg 입니다.'.format(data[0], data[1], data[2]))
